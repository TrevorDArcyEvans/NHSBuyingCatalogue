{
   "registration" : {
      "solution.version" : {
         "trim" : {},
         "in" : "body",
         "isLength" : {
            "errorMessage" : "Validation.Solution.Version.TooLong",
            "options" : {
               "max" : 10
            }
         }
      },
      "solution.description" : {
         "isEmpty" : {
            "negated" : true,
            "errorMessage" : "Validation.Solution.Description.Missing"
