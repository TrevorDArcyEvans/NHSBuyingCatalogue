            "errorMessage" : "Validation.Solution.Description.TooLong",
            "options" : {
               "max" : 300
            }
         },
         "in" : "body"
      },
      "solution.name" : {
         "isLength" : {
            "options" : {
               "max" : 60
            },
            "errorMessage" : "Validation.Solution.Name.TooLong"
         },
         "in" : "body",
         "isEmpty" : {
            "errorMessage" : "Validation.Solution.Name.Missing",
            "negated" : true
         },
         "trim" : {}
      }
   },
   "capabilities" : {
      "capabilities" : {
         "in" : "body",
         "isEmpty" : {
            "negated" : true,
            "errorMessage" : "Select at least one capability to continue"
         }
      }
   }
